{
  "name": "eleventy-starter",
  "version": "0.0.1",
  "author": "Dustin Whisman",
  "license": "ISC",
  "scripts": {
    "prestart": "run-p clean node-version",
    "prebuild": "run-p clean node-version",
    "start": "run-p eleventy:dev sass:dev postcss:watch",
    "build": "run-s eleventy:build sass:build postcss:build",
    "clean": "rimraf dist",
    "eleventy:dev": "NODE_ENV=development eleventy --serve",
    "eleventy:build": "NODE_ENV=production eleventy",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist/css/tmp",
    "sass:build": "sass --no-source-map src/scss:dist/css/tmp",
    "postcss:dev": "postcss dist/css/tmp/*.css -d dist/css",
    "postcss:build": "postcss --no-map dist/css/tmp/*.css -d dist/css",
    "postcss:watch": "chokidar \"**/tmp/*.css\" -c \"npm run postcss:dev\"",
    "node-version": "check-node-version --package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "^16.3.0",
    "npm": "^7.15.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "check-node-version": "^4.1.0",
    "chokidar-cli": "^2.1.0",
    "cssnano": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.1",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1"
  }
}
